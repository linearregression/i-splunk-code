<dashboard script="jquery.bxslider.js, autodiscover.js" stylesheet="jquery.bxslider.css">
  <label>imagepanel Clone</label>
  <description>A panel that shows random images based on a list of image filepaths</description>
  <row>
    <table>
      <title>testpanel</title>
      <searchString>|script python custompics</searchString>
      <option name="wrap">true</option>
      <option name="rowNumbers">false</option>
    </table>
    <html>
      <div id="imagepanel" 
	    class="splunk-manager splunk-searchmanager" 
		data-require="splunkjs/mvc/searchmanager" 
		data-options='{                         
			"app": "custom_simplexml_extensions",                      
			"search":  "|script python custompics",
		}'/>
      <div class="splunk-view" 
        data-require="app/custom_simplexml_extensions/components/imagepanel/imagepanel-random" 
        data-options='{
          "managerid": "imagepanel",
          "path": "custom_pics",
          "auto": "true",
          "mode": "vertical"     
         }'/>
    </html>
  </row>
</dashboard>