<form script="autodiscover.js, d3.v2.min.js">
  <label>chart</label>
  <description>checkout: https://github.com/i-splunk/i-splunk-code</description>
  <row>
    <html>
      <br />
      This is d3.js chart example <a href="http://d3js.org">http://d3js.org</a> for more information about chart implementation
      <br />
    </html>
  </row>
<row>
    <html>
      <h2>a simple d3 chart example</h2>
      <hr />
      <div id="chart-search" 
	    class="splunk-manager splunk-searchmanager" 
		data-require="splunkjs/mvc/searchmanager" 
		data-options='{                         
		  "app": "custom_simplexml_extensions",                      
		  "search":  {"type": "token_safe", "value": "|inputlookup chart.csv " }
		}'/>
      <div id="chart" 
		class="splunk-view" 
		data-require="app/custom_simplexml_extensions/components/chart/chart" 
		data-options='{
	      "managerid": "chart-search",
          "os": "os",
          "version": "version",
          "count": "count",
		  "height": 200
		}'/>
	<hr />
    </html>
</row>
</form>